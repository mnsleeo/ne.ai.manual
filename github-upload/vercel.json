{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/pages/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}